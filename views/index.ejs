<%- include partials/header.ejs %>
     
	  

 
	 
	 <div id="myCarousel" class="carousel slide" data-ride="carousel">
    <!-- Carousel indicators -->
    <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1"></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
    </ol>   
    <!-- Wrapper for carousel items -->
    <div class="carousel-inner">
        <div class="item active">
            <img src="http://localhost:3000/images/image1.jpg" alt="First Slide">
        </div>
        <div class="item">
            <img src="http://localhost:3000/images/image2.jpg" alt="Second Slide">
        </div>
        <div class="item">
            <img src="http://localhost:3000/images/image3.jpg" alt="Third Slide">
        </div>
    </div>
    <!-- Carousel controls -->
    <a class="carousel-control left" href="#Carousel" data-slide="prev">
        <span class="glyphicon glyphicon-chevron-left"></span>
    </a>
    <a class="carousel-control right" href="#Carousel" data-slide="next">
        <span class="glyphicon glyphicon-chevron-right"></span>
    </a>
</div>
	 
	 
	 
<div class="container-fluid">
    <div class="row">
        <div class="col-xs-4">
            <h2>HTML</h2>
            <p></p>
        </div>
        <div class="col-xs-8">
            <h2>CSS</h2>
        </div>   
    </div>
</div>
<h1><%= title %></h1>
    <p>Welcome to <%= message %></p>
  </body>
</html>
